<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	<managed-bean>
		<managed-bean-name>authentication</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.Authentication</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>visit</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.Visit</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createProject</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.CreateProject</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>inbox</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.Inbox</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selectItems</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.SelectItems</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>updateProject</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.UpdateProject</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>showHistory</managed-bean-name>
		<managed-bean-class>org.mseco.learning.jsf.projecttrack.ShowHistory</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
</faces-config>