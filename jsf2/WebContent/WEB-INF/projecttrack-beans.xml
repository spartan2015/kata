<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <converter>
  <converter-id>SelectItemToLocaleConverter</converter-id>
  <converter-class>org.mseco.learning.jsf.projecttrack.SelectItemToLocaleConverter</converter-class>
 </converter>
 <converter>
 	<converter-id>projectTypeConverter</converter-id>
 	<converter-class>org.mseco.learning.jsf.projecttrack.ProjectTypeConverter</converter-class>
 </converter>
 <converter>
 	<converter-id>artefactTypeConverter</converter-id>
 	<converter-class>org.mseco.learning.jsf.projecttrack.ArtefactTypeConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>authenticationBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.AuthenticationBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>visitBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.VisitBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>createProjectBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.CreateProjectBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
  	<property-name>visitBean</property-name>
  	<value>#{visitBean}</value>
  </managed-property>
  <managed-property>
  	<property-name>inboxBean</property-name>
  	<value>#{inboxBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>inboxBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.InboxBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
  	<property-name>visitBean</property-name>
  	<value>#{visitBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>selectItems</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.SelectItems</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>updateProjectBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.UpdateProjectBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>showHistoryBean</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.projecttrack.ShowHistoryBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>valueChangeApi</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.events.ValueChangeApi</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>actionApi</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.events.ActionApi</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>phaseApi</managed-bean-name>
  <managed-bean-class>org.mseco.learning.jsf.events.PhaseApi</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
</faces-config>
