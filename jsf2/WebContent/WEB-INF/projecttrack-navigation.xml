<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<navigation-rule>
		<from-view-id>/projecttrack/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success_readonly</from-outcome>
			<to-view-id>/projecttrack/general/details.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/projecttrack/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>inbox</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>show_all</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/projecttrack/protected/create.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/projecttrack/login.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{authenticationBean.logout}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/login.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{createProjectBean.create}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/protected/create.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/protected/inbox.jsp</from-view-id>
		<navigation-case>
			<from-action>#{inboxBean.details}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/general/details.jsp</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-action>#{inboxBean.approve}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/protected/approve.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{inboxBean.reject}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/protected/reject.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/general/show_all.jsp</from-view-id>
		<navigation-case>
			<from-action>#{inboxBean.details}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/projecttrack/general/details.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/protected/approve.jsp</from-view-id>
		<navigation-case>
			<from-action>#{updateProjectBean.approve}</from-action>
			<from-outcome>success_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.approve}</from-action>
			<from-outcome>success_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.cancel}</from-action>
			<from-outcome>cancel_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.cancel}</from-action>
			<from-outcome>cancel_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/projecttrack/protected/approve.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/protected/reject.jsp</from-view-id>
		<navigation-case>
			<from-action>#{updateProjectBean.reject}</from-action>
			<from-outcome>success_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.reject}</from-action>
			<from-outcome>success_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.cancel}</from-action>
			<from-outcome>cancel_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{updateProjectBean.cancel}</from-action>
			<from-outcome>cancel_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/projecttrack/protected/approve.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/protected/create.jsp</from-view-id>
		<navigation-case>
			<from-action>#{createProjectBean.add}</from-action>
			<from-outcome>success_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{createProjectBean.add}</from-action>
			<from-outcome>success_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/projecttrack/general/details.jsp</from-view-id>
		<navigation-case>
			<from-outcome>cancel_readonly</from-outcome>
			<to-view-id>/projecttrack/general/show_all.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel_readwrite</from-outcome>
			<to-view-id>/projecttrack/protected/inbox.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>